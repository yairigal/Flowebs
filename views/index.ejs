<!-- Login Modal -->
<div id="login-modal" class="modal input-modal">
    <div class="modal-content">
        <div class="center">
            <img id="modal-logo" class="circle" src="images/customer.png">
        </div>
        <div id="login-label" class="modal-title">Login:</div>
        <br>
        <form class="col s12">
            <div class="row">
                <div class="center form">
                    <div class="input-field">
                        <i id="person-login-icon" class="material-icons prefix">person</i>
                        <input placeholder="Username" id="login_username" type="text" class="validate">
                        <label for="first_name">Username:</label>
                    </div>
                    <!--<br>-->
                    <div class="input-field">
                        <i id="lock-login-icon" class="material-icons prefix">lock</i>
                        <input placeholder="Password" id="login_password" type="password" class="validate">
                        <label for="first_name">Password</label>
                    </div>
                    <br>
                    <a class="btn green waves-effect waves-light" onclick="login()">Login
                        <i class="material-icons right">send</i>
                    </a>
                    <br>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button class="modal-action waves-effect waves-green btn-flat">Register</button>
    </div>
</div>

<div id="edit-user-modal" class="modal input-modal">
    <div class="modal-content">
        <div class="center">
            <img id="modal-logo" class="circle" src="images/customer.png">
        </div>
        <div id="edit-user-modal-title" class="text-center modal-title">Edit User ID:</div>
        <br>
        <form class="col s12">
            <div class="row">
                <div class="center form">
                    <div class="input-field">
                        <i id="person-edit-icon" class="material-icons prefix">person</i>
                        <input placeholder="Username" id="edit-user-username" type="text" class="validate">
                        <label for="first_name">Username:</label>
