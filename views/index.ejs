                    <br>
                    <div class="input-field">
                        <i id="lock-edit-icon" class="material-icons prefix">lock</i>
                        <input placeholder="Password" id="edit-user-pwd" type="password" class="validate">
                        <label for="first_name">Password:</label>
                    </div>
                    <br>
                    <div class="input-field col s12">
                        <i id="type-edit-icon" class="material-icons prefix">assignment_ind</i>
                        <select id="edit-select-type">
                            <option value="1">Manager</option>
                            <option value="2">Worker</option>
                            <option value="3">Client</option>
                        </select>
                        <label>Type:</label>
                    </div>
                    <br>
                    <br>
                </div>
            </div>
        </form>

    </div>
    <div class="modal-footer">
        <button id="edit-user-send" class="green-text modal-action waves-effect waves-green btn-flat modal-footer-btn">Edit</button>
    </div>
</div>

